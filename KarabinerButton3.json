{
  "title": "command mouse paste",
  "rules": [
    {
      "description": "command+middle-button paste",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button3",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "key_code": "v",
              "modifiers": [
                "left_shift",
                "left_command"
              ]
            }
          ]
        }
      ]
    }
  ]
}
